{
  "name": "clueso-backend",
  "version": "1.0.0",
  "description": "Video processing AI platform backend with BullMQ, FFmpeg, and Deepgram integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --ignore uploads/ --exec ts-node src/server.ts",
    "worker": "ts-node src/workers/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/workers/index.js"
  },
  "keywords": [
    "video-processing",
    "ai",
    "bullmq",
    "ffmpeg",
    "deepgram"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@deepgram/sdk": "^3.9.0",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
